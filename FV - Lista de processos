SELECT DISTINCT

CODPLP AS PLANTA,CODPRC,DESCRABREV AS DESCRIÇÃO,
CODLOCALALMOXARIFE,CODLOCALMANUFATURA,
PERCDESVIOINF,PERCDESVIOSUP,LIBERADESVIO,ROTEIROHTML5

FROM TPRPRC
WHERE 1=1 and ativo = 's' and CODPLP = 8  

