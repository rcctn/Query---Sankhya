SELECT DISTINCT 

CODPROD AS ID_PRODUTO,
DESCRPROD AS NOME_PRODUTO,
PROD.CODGRUPOPROD AS ID_GRUPO,
DESCRGRUPOPROD AS NOME_GRUPO

FROM TGFPRO PROD

INNER JOIN TGFGRU GRUPO ON PROD.CODGRUPOPROD = GRUPO.CODGRUPOPROD


WHERE 1=1 and prod.ativo='s' and PROD.CODGRUPOPROD IN

(100101,100102,100103,100104,100105,100201,100202,100203,100204,100205,100401,110100,110200,110300,110400,110500,110600,110701,110702,120101,120102)
